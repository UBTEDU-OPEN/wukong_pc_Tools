UBXVideoPlayer
{
    background: #404040;
}

VideoPlayerContrlWidget
{
    background: transparent;
	margin: 0px;
	border: 0px;
	padding: 0px;
}

/***************播放器控制按钮start***************/
QPushButton#pbVideoPlay
{
    border-image: url(:/res/images/icon_play_nor.png);
    background-color: transparent;
    border: 0px; 
    min-height: 27px;
    max-height: 27px;
    min-width: 27px;
    max-width: 27px;
}

QPushButton#pbVideoPlay:hover
{
    border-image: url(:/res/images/icon_play_hl.png);
    background-color: transparent;
    border: 0px;  
}

QPushButton#pbVideoPlay:pressed
{
    border-image: url(:/res/images/icon_play_press.png);
    background-color: transparent;
    border: 0px;    
}

