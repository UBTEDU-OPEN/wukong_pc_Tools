QSlider::groove:horizontal {
    height: 1px;
    background-color: #10b5e5;
    left: 7px;
    right: 7px;
}

QSlider::handle:horizontal {
    border-image: url(:/res/images/icon_play_position.png);
    width: 14px;
    margin: -7px -7px -7px -7px;
}

