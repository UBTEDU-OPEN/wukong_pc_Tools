/************UBXMotionEditorLayout Widget begin************/
QWidget#CUBXMotionEditorLayout
{
    background-color: #eaeff1;
}

QWidget#CUIMainLayerLayout
{
    background-color: #eaeff1;
}

QScrollBar:horizontal {
    border: none;
    background: #eaeff1;
    height: 20px;
}

QScrollBar::handle:horizontal
{
    background: rgb(32, 179, 241);
    min-width: 20px;
    height: 10px;
    margin: 5px 1px 5px 1px;
    border-radius: 3px;
    border: none;
}

QScrollBar::add-line:horizontal
{
    width: 0px;
    height: 0px;
}

QScrollBar::sub-line:horizontal
{
    width: 0px;
    height: 0px;
}


QScrollBar:vertical
{
    border: none;
    background: #eaeff1;
    width: 20px;
}

QScrollBar::handle:vertical
{
    background: rgb(32, 179, 241);
    min-height: 20px;
    width: 10px;
    margin: 1px 5px 1px 5px;
    border-radius: 3px;
    border: none;
}

QScrollBar::add-line:vertical
{
    width: 0px;
    height: 0px;
}

QScrollBar::sub-line:vertical
{
    width: 0px;
    height: 0px;
}

/************UBXMotionEditorLayout Widget end************/

